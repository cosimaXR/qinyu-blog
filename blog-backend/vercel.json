{
  "functions": {
    "src/index.js": {
      "runtime": "node18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/index.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://www.qinyu.blog" }
      ]
    }
  ]
} 